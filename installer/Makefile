.PHONY: build chkconfig preconfig buildfs clean

include ../bsp/classes/rrsdk_local.class
include $(DEVDIR)/bsp/mach/Make.conf

patch::

build:

chkconfig: 

preconfig:
	$(V) echo -e "#This file is autogenerated by makefile, don't modify\n\n" > ConfigExtra
ifneq ($(strip $(INSTALLER)),)
	$(V) echo -e "source installer/$(INSTALLER)/Config\n" >> ConfigExtra
endif

buildfs:

unpatch::

clean: 
	$(V) rm -f ConfigExtra
ifneq ($(strip $(INSTALLER)),)
	$(MAKE) -C $(INSTALLER) $(MAKE_CALL_PARAMS) $@
endif

